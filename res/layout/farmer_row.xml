<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/farmer_row"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content">
	<TextView 
		android:id="@+id/txt_row_id" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView 
		android:id="@+id/txt_farmer_id" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView 
		android:id="@+id/txt_farmer_fname" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView 
		android:id="@+id/txt_farmer_lname" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView 
		android:id="@+id/txt_farmer_size" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
</LinearLayout>
